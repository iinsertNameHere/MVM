%define print_char 0
%define print_f64  1
%define print_i64  2
%define print_u64  3
%define print_ptr  4
%define alloc      5
%define free       6

jmp stdlib_end

%define NL 13

println_char:
    swap 1
    ncall print_char
    push NL
    ncall print_char
    ret

println_f64:
    swap 1
    ncall print_f64
    push NL
    ncall print_char
    ret

println_i64:
    swap 1
    ncall print_i64
    push NL
    ncall print_char
    ret

println_u64:
    swap 1
    ncall print_u64
    push NL
    ncall print_char
    ret

println_ptr:
    swap 1
    ncall print_ptr
    push NL
    ncall print_char
    ret

stdlib_end: