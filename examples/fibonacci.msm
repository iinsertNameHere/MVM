jmp main

print_i64_ln:
    swap 1
    ncall 2
    push 13 # New line ASCII code
    ncall 0 # Print Char
    ret

# Main Loop
main:
    push 0
    push 1
    push 30 # Iterations

loop:
    swap 2
    dup 0
    call print_i64_ln
    dup 1
    plusi
    swap 1
    swap 2
    push 1
    minusi

    dup 0
    push 0
    eq
    not
    jmpif loop
hlt