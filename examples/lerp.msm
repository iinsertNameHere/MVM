%include "../msmlib/stdlib.mlb"

jmp main

; Iterations
%define I 10.0

lerp_f64:
    dup 3
    dup 3
    dup 1
    minusf
    dup 3
    multf
    plusf

    ; cleanup phase
    swap 2
    drop
    swap 2
    drop
    swap 2
    drop
    ret

main:
    push 69.0
    push 420.0
    push 0.0
    push 1.0
    push I
    divf
loop:
    dup 3
    dup 3
    dup 3
    call lerp_f64
    call println_f64

    swap 1
    dup 1
    plusf
    swap 1

    dup 1
    push 1.0
    geeqf

    jmpif loop
hlt